{
  "name": "stockhub",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "proxy": "http://localhost:3000",
  "scripts": {
    "start": "npm run dev",
    "start:host": "vite --host",
    "build:prod": "tsc && vite build --mode production",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest",
    "test:ci":"jest --config=jest.ci.config.cjs --passWithNoTests --detectOpenHandles --forceExit",
    "test:coverage": "COVERAGE=true jest --config=jest.ci.config.cjs --passWithNoTests --detectOpenHandles --forceExit",
    "test:watch": "jest --watchAll"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "autoprefixer": "^10.4.19",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "postcss": "^8.4.38",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.3.0",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-modal": "^3.16.3",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "vite": "5.2.6"
  }
}
